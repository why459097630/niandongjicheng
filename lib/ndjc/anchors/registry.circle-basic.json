      "NDJC:USE_APP_BUNDLE": "true",
      "NDJC:BUILD_META_RUNID": "",
      "NDJC:FEATURE_TOGGLE_MIN": "baseline",
      "NDJC:DEBUG_LOG_ENABLED": "false",

      "NDJC:RUN_ID": "",
      "NDJC:LOG_LEVEL": "info",
      "NDJC:METRICS_ENABLED": "false",
      "NDJC:CRASHLYTICS_ENABLED": "false",

      "NDJC:EXPERIMENT_BUCKET": ""
    },
    "list": {
      "LIST:ROUTES": ["home", "detail/{id}", "post"],
      "LIST:PROGUARD_EXTRA": [],
      "LIST:PACKAGING_RULES": [],
      "LIST:POST_FIELDS": [],
      "LIST:COMMENT_FIELDS": [],
      "LIST:ABI_SPLITS": [],
      "LIST:PLURAL_STRINGS": [],
      "LIST:COMPONENT_STYLES": [],
      "LIST:REMOTE_FLAGS": [],
      "LIST:DEEPLINK_PATTERNS": []
    },
    "gradle": {
      "applicationId": "com.ndjc.demo.core",
      "resConfigs": ["en", "zh-rCN", "zh-rTW"],
      "permissions": []
    }
  },

  "placeholders": {
    "text": {
      "NDJC:APP_LABEL": "__APP_LABEL__",
      "NDJC:HOME_TITLE": "__HOME_TITLE__",
      "NDJC:PRIMARY_BUTTON_TEXT": "__PRIMARY_BUTTON__",
      "NDJC:THEME_NAME": "__THEME__",
      "NDJC:PRIVACY_POLICY": "__PRIVACY_URL__"
    },
    "block": {
      "BLOCK:HOME_HEADER": "<!-- __HOME_HEADER__ -->",
      "BLOCK:HOME_BODY": "<!-- __HOME_BODY__ -->",
      "BLOCK:HOME_ACTIONS": "<!-- __HOME_ACTIONS__ -->",
      "BLOCK:THEME_COLORS": "{ /* __THEME_COLORS__ */ }",
      "BLOCK:EMPTY_STATE": "<!-- __EMPTY__ -->",
      "BLOCK:ERROR_STATE": "<!-- __ERROR__ -->"
    },
    "list": {
      "LIST:ROUTES": ["home"],
      "LIST:DEEPLINK_PATTERNS": [],
      "LIST:PROGUARD_EXTRA": [],
      "LIST:PACKAGING_RULES": []
    },
    "if": {},
    "hook": {
      "HOOK:BEFORE_BUILD": "",
      "HOOK:AFTER_BUILD": ""
    },
    "resources": {},
    "gradle": {
      "applicationId": "com.ndjc.demo.core",
      "resConfigs": ["en"],
      "permissions": []
    }
  },

  "valueFormat": {
    "text": {
      "NDJC:PACKAGE_NAME": { "regex": "^[a-zA-Z][a-zA-Z0-9_]*(\\.[a-zA-Z][a-zA-Z0-9_]*)+$" },
      "NDJC:APP_LABEL": { "minLen": 1, "maxLen": 40 },
      "NDJC:HOME_TITLE": { "minLen": 1, "maxLen": 40 },
      "NDJC:PRIMARY_BUTTON_TEXT": { "minLen": 1, "maxLen": 24 },
      "NDJC:ANIM_DURATION_MS": { "regex": "^[0-9]{1,5}$" },
      "NDJC:PAGING_SIZE": { "regex": "^[0-9]{1,3}$" },
      "NDJC:LOG_LEVEL": { "enum": ["trace", "debug", "info", "warn", "error"] }
    },
    "block": {
      "BLOCK:THEME_COLORS": { "maxLen": 20000 }
    },
    "list": {
      "LIST:ROUTES": { "itemRegex": "^[a-z0-9_\\-/{}}]{1,32}$", "minItems": 1, "maxItems": 64 },
      "LIST:DEEPLINK_PATTERNS": { "itemRegex": "^[a-z0-9_\\-/{\\.:}]+$", "maxItems": 64 }
    },
    "if": {},
    "hook": {},
    "resources": {},
    "gradle": {
      "applicationId": { "regex": "^[a-z][a-z0-9_]*(\\.[a-z][a-z0-9_]*)+$" },
      "resConfigs": { "itemRegex": "^[a-z]{2}(-r[A-Z]{2})?$" },
      "permissions": { "itemRegex": "^android\\.permission\\.[A-Z_]+$" }
    }
  }
}
