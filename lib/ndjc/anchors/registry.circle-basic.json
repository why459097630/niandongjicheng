  "resources": [],

  "hints": {
    "NDJC:PACKAGE_NAME": { "group": "text", "type": "string:packageId", "desc": "Android applicationId / 包名", "example": "com.ndjc.demo.core", "rules": { "pattern": "^[a-z][a-z0-9_]*(\\.[a-z][a-z0-9_]*)+$" } },
    "NDJC:APP_LABEL": { "group": "text", "type": "string", "desc": "应用显示名称", "example": "NDJC App", "rules": { "minLen": 3, "maxLen": 30 } },
    "NDJC:HOME_TITLE": { "group": "text", "type": "string", "desc": "首页标题", "example": "Home", "rules": { "minLen": 3 } },
    "NDJC:PRIMARY_BUTTON_TEXT": { "group": "text", "type": "string", "desc": "首页主按钮文案", "example": "Create", "rules": { "minLen": 3 } },

    "NDJC:THEME_NAME": { "group": "text", "type": "enum", "desc": "主题模式", "example": "Light", "rules": { "enum": ["Light","Dark","System"] } },
    "NDJC:DARK_MODE": { "group": "text", "type": "boolean", "desc": "启用深色模式", "example": true },

    "NDJC:FONT_FAMILY_PRIMARY": { "group": "text", "type": "string", "desc": "主字体族", "example": "OpenSans", "rules": { "minLen": 3 } },
    "NDJC:FONT_FAMILY_SECONDARY": { "group": "text", "type": "string", "desc": "次字体族", "example": "Montserrat", "rules": { "minLen": 3 } },

    "NDJC:I18N_ENABLED": { "group": "text", "type": "boolean", "desc": "是否启用多语言", "example": true },
    "NDJC:ANIM_ENABLED": { "group": "text", "type": "boolean", "desc": "是否启用动画", "example": true },
    "NDJC:ANIM_DURATION_MS": { "group": "text", "type": "number", "desc": "动画时长(ms)", "example": 300, "rules": { "min": 50, "max": 2000 } },

    "NDJC:LAYOUT_DENSITY": { "group": "text", "type": "enum", "desc": "版式密度", "example": "mdpi", "rules": { "enum": ["ldpi","mdpi","hdpi","xhdpi","xxhdpi"] } },
    "NDJC:DATA_SOURCE": { "group": "text", "type": "string:url", "desc": "远端数据源根URL", "example": "https://api.example.com" },
    "NDJC:SEED_ENABLED": { "group": "text", "type": "boolean", "desc": "启用种子数据", "example": true },
    "NDJC:FEED_SORT": { "group": "text", "type": "enum", "desc": "列表排序", "example": "date", "rules": { "enum": ["date","popularity","rating"] } },
    "NDJC:PAGING_SIZE": { "group": "text", "type": "number", "desc": "分页大小", "example": 10, "rules": { "min": 1, "max": 200 } },

    "NDJC:OFFLINE_POLICY": { "group": "text", "type": "enum", "desc": "离线策略", "example": "cache", "rules": { "enum": ["cache","network","mixed"] } },
    "NDJC:PRIVACY_POLICY": { "group": "text", "type": "string:https-url", "desc": "隐私政策链接", "example": "https://example.com/privacy" },
    "NDJC:DRAFT_ENABLED": { "group": "text", "type": "boolean", "desc": "启用草稿功能", "example": true },
    "NDJC:USE_APP_BUNDLE": { "group": "text", "type": "boolean", "desc": "构建 AAB", "example": true },

    "NDJC:BUILD_META:RUNID": { "group": "text", "type": "string", "desc": "本次构建运行ID（若生成此锚点，其值必须与 metadata.NDJC:BUILD_META:RUNID 一致）", "example": "run_20251022_001", "rules": { "minLen": 3 } },
    "NDJC:FEATURE_TOGGLE:MIN": { "group": "text", "type": "number", "desc": "最小功能开关级别", "example": 1 },
    "NDJC:DEBUG_LOG_ENABLED": { "group": "text", "type": "boolean", "desc": "调试日志开", "example": true },
    "NDJC:LOG_LEVEL": { "group": "text", "type": "enum", "desc": "日志级别", "example": "INFO", "rules": { "enum": ["DEBUG","INFO","WARN","ERROR"] } },
    "NDJC:CRASHLYTICS_ENABLED": { "group": "text", "type": "boolean", "desc": "崩溃上报开", "example": true },
    "NDJC:EXPERIMENT_BUCKET": { "group": "text", "type": "string", "desc": "实验分桶名", "example": "exp-001", "rules": { "minLen": 3 } },

    "NDJC:THEME_COLORS": { "group": "text", "type": "object", "desc": "主题主副色", "example": { "primary": "#7C3AED", "secondary": "#10B981" } },
    "NDJC:TYPOCASE_OVERRIDES": { "group": "text", "type": "string", "desc": "文案大小写覆盖", "example": "none" },
    "NDJC:STRINGS_EXTRA": { "group": "text", "type": "object", "desc": "额外字符串资源", "example": { "extra": "value" } },

    "NDJC:ROUTE_HOME": { "group": "text", "type": "string:routeId", "desc": "首页路由ID", "example": "home" },
    "NDJC:ROUTE_DETAIL": { "group": "text", "type": "string:routeId", "desc": "详情页路由ID", "example": "detail" },

    "BLOCK:HOME_HEADER": { "group": "block", "type": "block:compose", "desc": "首页头部可替换 UI 片段", "example": "@Composable fun HomeHeader(){ Row{ Text(\"NDJC App\") ; Button(onClick={}){ Text(\"Create\") } } }", "rules": { "mustCompile": true } },
    "BLOCK:HOME_BODY": { "group": "block", "type": "block:compose", "desc": "首页主体内容片段", "example": "Column { Text(\"Welcome\") }" },
    "BLOCK:HOME_ACTIONS": { "group": "block", "type": "block:compose", "desc": "首页操作区片段", "example": "Row { Button(onClick={}){ Text(\"Go\") } }" },
    "BLOCK:SPLASH_CONTENT": { "group": "block", "type": "block:compose", "desc": "启动页片段", "example": "@Composable fun Splash(){ Box { Text(\"Loading…\") } }" },
    "BLOCK:EMPTY_STATE": { "group": "block", "type": "block:compose", "desc": "空数据态片段", "example": "Column { Text(\"No data\") }" },
    "BLOCK:ERROR_STATE": { "group": "block", "type": "block:compose", "desc": "错误态片段", "example": "Column { Text(\"Error\") }" },
    "BLOCK:DEBUG_PANEL": { "group": "block", "type": "block:compose", "desc": "调试面板片段", "example": "Column { Text(\"Debug\") }" },
    "BLOCK:HEADER_AD_SLOT": { "group": "block", "type": "block:compose", "desc": "头部广告位片段", "example": "Box { Text(\"AD\") }" },
    "BLOCK:USER_PROFILE_CARD": { "group": "block", "type": "block:compose", "desc": "用户卡片片段", "example": "@Composable fun ProfileCard(){ Row{ Text(\"User\") } }" },
    "BLOCK:SPONSORED_CARD": { "group": "block", "type": "block:compose", "desc": "赞助卡片片段", "example": "@Composable fun Sponsored(){ Card{ Text(\"Sponsor\") } }" },
    "BLOCK:PROFILE_SECTION": { "group": "block", "type": "block:compose", "desc": "个人资料区片段", "example": "Column { Text(\"Profile\") }" },
    "BLOCK:SETTINGS_SECTION": { "group": "block", "type": "block:compose", "desc": "设置页片段", "example": "Column { Text(\"Settings\") }" },
    "BLOCK:SETTINGS_SECTION:EXTRA": { "group": "block", "type": "block:compose", "desc": "设置页扩展片段", "example": "Column { Text(\"Extra\") }" },

    "LIST:ROUTES": { "group": "list", "type": "list<string:routeId>", "desc": "路由ID列表", "example": ["home","detail"], "rules": { "itemPattern": "^[a-z][a-z0-9_-]*$", "minItems": 1 } },
    "LIST:DEPENDENCY_SNIPPETS": { "group": "list", "type": "list<string>", "desc": "额外 Gradle 依赖行", "example": ["implementation 'androidx.core:core-ktx:1.10.0'"] },
    "LIST:PROGUARD_EXTRA": { "group": "list", "type": "list<string>", "desc": "额外 Proguard 规则", "example": ["-keep class com.ndjc.** { *; }"] },
    "LIST:PACKAGING_RULES": { "group": "list", "type": "list<string>", "desc": "打包规则(排除/合并)", "example": ["resources.exclude('META-INF/DEPENDENCIES')"] },
    "LIST:POST_FIELDS": { "group": "list", "type": "list<string>", "desc": "发布/表单字段", "example": ["title","description"] },
    "LIST:PLURAL_STRINGS": { "group": "list", "type": "list<string>", "desc": "复数化字符串键", "example": ["items_count"] },
    "LIST:COMPONENT_STYLES": { "group": "list", "type": "list<string>", "desc": "组件风格", "example": ["material3"] },
    "LIST:DEEPLINK_PATTERNS": { "group": "list", "type": "list<string:url|pattern>", "desc": "DeepLink URL 模式", "example": ["https://example.com/home"] },
    "LIST:NETWORK_CLEAR_TEXT": { "group": "list", "type": "list<string:domain>", "desc": "需允许 HTTP 明文的域名", "example": ["api.example.com"] },
    "LIST:RES_CONFIGS_OVERRIDE": { "group": "list", "type": "list<string:locale>", "desc": "追加语言资源配置", "example": ["en","zh-rCN"], "rules": { "itemPattern": "^[a-z]{2}(-[A-Z]{2,3})?$" } },

    "IF:NAV_TRANSITIONS": { "group": "if", "type": "boolean", "desc": "启用导航转场", "example": true },
    "IF:BUILD_SUMMARY": { "group": "if", "type": "boolean", "desc": "构建结束输出摘要", "example": true },
    "IF:NATIVE_SPLITS": { "group": "if", "type": "boolean", "desc": "启用 ABI 拆分", "example": false },
    "IF:AFTER_BUILD": { "group": "if", "type": "boolean", "desc": "构建后执行 hook", "example": true },
    "IF:PRE_COMMIT": { "group": "if", "type": "boolean", "desc": "提交前执行 hook", "example": false },
    "IF:AFTER_INSTALL": { "group": "if", "type": "boolean", "desc": "安装后执行 hook", "example": true },
    "IF:BEFORE_BUILD": { "group": "if", "type": "boolean", "desc": "构建前执行 hook", "example": true },
    "IF:POST_INJECT": { "group": "if", "type": "boolean", "desc": "模板注入后执行 hook", "example": true },
    "IF:BEFORE_BUNDLE": { "group": "if", "type": "boolean", "desc": "打 AAB 前执行 hook", "example": false },
    "IF:AFTER_BUNDLE": { "group": "if", "type": "boolean", "desc": "打 AAB 后执行 hook", "example": false },

    "HOOK:INTENT_SHARE": { "group": "hook", "type": "hook:enum", "desc": "分享意图 hook", "example": "enabled", "rules": { "enum": ["noop","enabled","emit_event:SHARE"] } },
    "HOOK:FILE_PROVIDER": { "group": "hook", "type": "hook:enum", "desc": "文件提供器 hook", "example": "enabled", "rules": { "enum": ["noop","enabled"] } },
    "HOOK:PERMISSIONS:ON_REQUEST": { "group": "hook", "type": "hook:enum", "desc": "动态权限请求 hook", "example": "emit_event:PERM_REQUESTED", "rules": { "enum": ["noop","enabled","emit_event:PERM_REQUESTED"] } },
    "HOOK:PRE_COMMIT:HOOK": { "group": "hook", "type": "hook:enum", "desc": "提交前 hook 实现", "example": "noop", "rules": { "enum": ["noop","enabled"] } },
    "HOOK:POST_BUILD:HOOK": { "group": "hook", "type": "hook:enum", "desc": "构建后 hook 实现", "example": "emit_event:BUILD_DONE", "rules": { "enum": ["noop","enabled","emit_event:BUILD_DONE"] } },
    "HOOK:AFTER_INSTALL:HOOK": { "group": "hook", "type": "hook:enum", "desc": "安装后 hook 实现", "example": "enabled", "rules": { "enum": ["noop","enabled"] } },

    "gradle.applicationId": { "group": "gradle", "type": "string:packageId", "desc": "Gradle applicationId（应与 NDJC:PACKAGE_NAME 一致）", "example": "com.ndjc.demo.core", "rules": { "pattern": "^[a-z][a-z0-9_]*(\\.[a-z][a-z0-9_]*)+$" } },
    "gradle.resConfigs": { "group": "gradle", "type": "list<string:locale>", "desc": "包含的资源语言", "example": ["en","zh-rCN"], "rules": { "itemPattern": "^[a-z]{2}(-r[A-Z]{2,3})?$", "minItems": 1 } },
    "gradle.permissions": { "group": "gradle", "type": "list<string:androidPermission>", "desc": "Android 权限清单", "example": ["android.permission.INTERNET"], "rules": { "itemPattern": "^android\\.permission\\.[A-Z_]+$", "minItems": 1 } }
  },

