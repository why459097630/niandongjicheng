      "BLOCK:NOTIFICATIONS_SCREEN": { "minLen": 40, "regex": "@Composable|LazyColumn\\(" },
      "BLOCK:EDIT_PROFILE_SCREEN": { "minLen": 40, "regex": "@Composable|TextField\\(" },
      "BLOCK:SEARCH_SCREEN": { "minLen": 40, "regex": "@Composable|TextField\\(|LazyColumn\\(" },
      "BLOCK:MEDIA_PICKER": { "minLen": 30, "regex": "@Composable|Image\\(|Text\\(" }
    }
  }
}
