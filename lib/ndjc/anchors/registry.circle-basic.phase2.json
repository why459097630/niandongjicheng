      "BLOCK:DEBUG_PANEL": { "minLen": 40, "regex": "@Composable|Text\\(|Button\\(|Row\\(|Column\\(|Image\\(" },
      "BLOCK:HEADER_AD_SLOT": { "minLen": 40, "regex": "@Composable|Text\\(|Button\\(|Row\\(|Column\\(|Image\\(" },
      "BLOCK:USER_PROFILE_CARD": { "minLen": 40, "regex": "@Composable|Text\\(|Button\\(|Row\\(|Column\\(|Image\\(" },
      "BLOCK:SPONSORED_CARD": { "minLen": 40, "regex": "@Composable|Text\\(|Button\\(|Row\\(|Column\\(|Image\\(" },
      "BLOCK:PROFILE_SECTION": { "minLen": 40, "regex": "@Composable|Text\\(|Button\\(|Row\\(|Column\\(|Image\\(" },
      "BLOCK:SETTINGS_SECTION": { "minLen": 40, "regex": "@Composable|Text\\(|Button\\(|Row\\(|Column\\(|Image\\(" },
      "BLOCK:SETTINGS_SECTION:EXTRA": { "minLen": 40, "regex": "@Composable|Text\\(|Button\\(|Row\\(|Column\\(|Image\\(" },
      "BLOCK:POST_COMPOSER": { "minLen": 40, "regex": "@Composable|TextField\\(|Text\\(|Button\\(" },
      "BLOCK:POST_CARD": { "minLen": 40, "regex": "@Composable|Text\\(|Row\\(" },
      "BLOCK:FEED_LIST": { "minLen": 40, "regex": "@Composable|LazyColumn\\(|Column\\(" },
      "BLOCK:POST_DETAIL": { "minLen": 40, "regex": "@Composable|Column\\(|Text\\(" },
      "BLOCK:COMMENTS_SCREEN": { "minLen": 40, "regex": "@Composable|Column\\(|TextField\\(|LazyColumn\\(" },
      "BLOCK:COMMENT_ITEM": { "minLen": 40, "regex": "@Composable|Row\\(|Text\\(" },
